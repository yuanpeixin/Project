<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.peixin.hangout"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="8" />

    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name="com.example.hangout.FrontpageActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.hangout.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.example.hangout.MAINACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.hangout.Menu"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Light" >
            <intent-filter>
                <action android:name="com.example.hangout.MENU" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.hangout.Text"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.hangout.DisplayMessageActivity"
            android:label="@string/title_activity_display_message" >
        </activity>
        <activity
            android:name="com.example.hangout.SendData"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.hangout.ReceiveData"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.hangout.AboutUs"
            android:label="About Us"
            android:theme="@android:style/Theme.Dialog" >
        </activity>
        <activity
            android:name="com.example.hangout.Prefs"
            android:label="Preferences" >
        </activity>
        <activity
            android:name="com.example.hangout.GFX"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.hangout.GFXSurface"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.hangout.SoundStuff"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.hangout.Sliding"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.hangout.Tabs"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.hangout.SimpleBrowser"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.hangout.Flipper"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.hangout.Sharedpreference"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.hangout.InternalData"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.hangout.ExternalData"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.hangout.SQLite"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.hangout.SQLView"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.hangout.Accelerate"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.hangout.HttpExample"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.hangout.WeatherXMLParse"
            android:label="@string/app_name" >
        </activity>

        <receiver
            android:name="com.example.hangout.PointlessWidget"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE" />
            </intent-filter>

            <meta-data
                android:name="android.appwidget.provider"
                android:resource="@xml/widget_stuff" />
        </receiver>

        <activity
            android:name="com.example.hangout.WidgetConfig"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_CONFIGURE" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.hangout.GLExample"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.hangout.GLCubEX"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.hangout.Voice"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.hangout.TextVoice"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.hangout.StatusBar"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.hangout.SeekBarVolume"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.google.android.gms.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"
            android:theme="@android:style/Theme.Translucent" />
    </application>

</manifest>